	<CMD_RMLOGLEVEL>false</CMD_RMLOGLEVEL>
	<CMD_RMFORCEREINDEX>false</CMD_RMFORCEREINDEX>
	<CMD_EXPORTPATH></CMD_EXPORTPATH>
	<CMD_RMINDEXER>false</CMD_RMINDEXER>
	<CMD_INDEXERVALUE>@indexerA</CMD_INDEXERVALUE>
	<CMD_RMSECURITY>false</CMD_RMSECURITY>
	<CMD_SECURITYVALUE>Sinequa</CMD_SECURITYVALUE>
	<CMD_CUSTOM>true</CMD_CUSTOM>
	<CMD_CUSTOMELEMENT>/Sinequa/IndexPdfAttachments</CMD_CUSTOMELEMENT>
	<CMD_CUSTOMELEMENTVALUE>false</CMD_CUSTOMELEMENTVALUE>
	<CMD_RMCUSTOM>true</CMD_RMCUSTOM>
	<CMD_CUSTOMVALUE>true</CMD_CUSTOMVALUE>
</Sinequa>