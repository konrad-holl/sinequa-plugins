<?xml version="1.0" encoding="utf-8"?>
<Sinequa>
	<Revision>2</Revision>
	<Description>Check sinequa configurations for good practices</Description>
	<CommandType>plugin</CommandType>
	<Identity></Identity>
	<Plugin>ConfigChecker/Sinequa.Plugin.ConfigChecker</Plugin>
	<System>
		<LogLevel></LogLevel>
		<LogInConsole>true</LogInConsole>
	</System>
	<Force32Bits>false</Force32Bits>
	<StatusMaxOk></StatusMaxOk>
	<RuntimeToRunWith></RuntimeToRunWith>
	<Java>
		<Server>true</Server>
		<Xms></Xms>
		<Xmx></Xmx>
		<DisablePerfData>true</DisablePerfData>
		<OtherOptions></OtherOptions>
	</Java>
	<CMD_SOURCEPATH>E:\configtest\</CMD_SOURCEPATH>
	<CMD_LOGLEVEL>true</CMD_LOGLEVEL>
	<CMD_INDEXER>true</CMD_INDEXER>
	<CMD_FORCEREINDEX>true</CMD_FORCEREINDEX>
